<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lzf.tfirst.sys_user.mapper.SysUserMapper">

    <select id="findByUserName" resultType="com.lzf.tfirst.sys_user.entity.SysUser">
        select * from sys_user where user_name = #{username}
    </select>
    <select id="selectAll" resultType="com.lzf.tfirst.sys_user.entity.SysUser">
        select * from sys_user
    </select>
</mapper>
